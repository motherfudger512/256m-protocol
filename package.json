{
  "name": "256m-protocol",
  "version": "0.1.0",
  "description": "256M Insurance Protocol",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "./run-all-tests.sh",
    "test:core": "anchor test --skip-local-validator tests/liquidity-pool.test.ts",
    "test:stress": "anchor test --skip-local-validator tests/liquidity-pool-advanced.test.ts",
    "test:quick": "anchor test --skip-deploy",
    "build": "anchor build",
    "deploy:devnet": "./deploy-devnet.sh",
    "initialize-devnet": "ts-node scripts/initialize-devnet.ts",
    "test-journey": "ts-node scripts/test-customer-journey.ts",
    "validator:start": "solana-test-validator > validator.log 2>&1 &",
    "validator:stop": "pkill solana-test-validator"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  }
}